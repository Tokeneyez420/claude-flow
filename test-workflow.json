{
  "name": "Test Workflow",
  "version": "1.0.0",
  "description": "Simple test workflow for automation testing",
  "variables": {
    "test_param": "test_value"
  },
  "agents": [
    {
      "id": "test_agent",
      "type": "tester", 
      "name": "Test Agent",
      "config": {
        "capabilities": ["testing", "validation"]
      }
    }
  ],
  "tasks": [
    {
      "id": "test_task_1",
      "name": "First Test Task",
      "type": "test",
      "description": "Execute first test task",
      "assignTo": "test_agent",
      "timeout": 300
    },
    {
      "id": "test_task_2", 
      "name": "Second Test Task",
      "type": "test",
      "description": "Execute second test task",
      "assignTo": "test_agent",
      "depends": ["test_task_1"],
      "timeout": 300
    }
  ],
  "settings": {
    "maxConcurrency": 1,
    "timeout": 600,
    "failurePolicy": "continue"
  }
}