{
  "name": "basic-test-suite",
  "version": "1.0.0",
  "description": "Comprehensive TDD test suite following London School methodology",
  "main": "src/index.js",
  "scripts": {
    "test": "jest --coverage",
    "test:unit": "jest tests/unit --coverage",
    "test:integration": "jest tests/integration --coverage",
    "test:e2e": "jest tests/e2e --coverage",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage --coverageReporters=html,text,lcov",
    "lint": "eslint src tests",
    "lint:fix": "eslint src tests --fix"
  },
  "keywords": ["tdd", "testing", "london-school", "jest"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^20.11.24",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "supertest": "^6.3.4"
  },
  "dependencies": {
    "express": "^4.18.3"
  }
}