{
  "name": "Concurrent Test Workflow",
  "description": "Test workflow with multiple concurrent tasks",
  "version": "1.0.0",
  "type": "test",
  "agents": [
    {
      "id": "search_agent",
      "name": "Search Agent",
      "type": "researcher",
      "config": {
        "capabilities": ["web-search", "analysis"]
      }
    },
    {
      "id": "coder_agent",
      "name": "Code Agent",
      "type": "coder",
      "config": {
        "capabilities": ["coding", "testing"]
      }
    },
    {
      "id": "analyzer_agent",
      "name": "Analysis Agent",
      "type": "analyst",
      "config": {
        "capabilities": ["data-analysis", "reporting"]
      }
    }
  ],
  "tasks": [
    {
      "id": "task1",
      "name": "Search Task",
      "type": "search",
      "description": "Search for information",
      "assignTo": "search_agent",
      "claudePrompt": "Search for the latest best practices in API design. List 5 key principles."
    },
    {
      "id": "task2",
      "name": "Code Task",
      "type": "implementation",
      "description": "Create code example",
      "assignTo": "coder_agent",
      "claudePrompt": "Create a simple Python function that demonstrates error handling best practices."
    },
    {
      "id": "task3",
      "name": "Analysis Task",
      "type": "analysis",
      "description": "Analyze patterns",
      "assignTo": "analyzer_agent",
      "claudePrompt": "Analyze the benefits of microservices architecture. List 3 pros and 3 cons.",
      "depends": ["task1"]
    }
  ],
  "settings": {
    "failurePolicy": "continue"
  }
}