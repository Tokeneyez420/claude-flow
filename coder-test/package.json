{
  "name": "coder-agent-test",
  "version": "1.0.0",
  "description": "Comprehensive test suite for coder agent functionality",
  "main": "index.ts",
  "scripts": {
    "test": "jest --coverage",
    "test:watch": "jest --watch",
    "test:unit": "jest test-coder-agent.ts",
    "test:integration": "ts-node integration-test.ts",
    "test:performance": "ts-node performance-benchmark.ts",
    "test:all": "npm run test:unit && npm run test:integration && npm run test:performance",
    "benchmark": "ts-node performance-benchmark.ts",
    "lint": "eslint . --ext .ts",
    "typecheck": "tsc --noEmit"
  },
  "keywords": [
    "coder",
    "agent",
    "testing",
    "swarm",
    "claude-flow"
  ],
  "author": "Claude Flow Team",
  "license": "MIT",
  "dependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  },
  "devDependencies": {
    "@jest/globals": "^29.0.0",
    "@types/jest": "^29.0.0",
    "eslint": "^8.0.0",
    "jest": "^29.0.0",
    "ts-jest": "^29.0.0",
    "ts-node": "^10.0.0"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/node_modules/**",
      "!**/dist/**"
    ],
    "coverageThreshold": {
      "global": {
        "branches": 80,
        "functions": 80,
        "lines": 80,
        "statements": 80
      }
    }
  }
}